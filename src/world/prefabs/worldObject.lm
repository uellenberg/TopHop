//A doa for main world objects.

createPrefab!("world_object");

//Set this object to be a doa.
useBehaviorPrefab!("world_object", "doa", "0");
//Allow this object to be collided with.
useBehaviorPrefab!("world_object", "square_collider");
//Make this object render.
useBehaviorPrefab!("world_object", "square");